{
  "compilerOptions": {
    "target": "ES5",
    "module": "CommonJS",
    "jsx": "preserve",
    "noImplicitAny": false,
    "skipLibCheck": true,
    "declaration": true,
    "strictNullChecks": false,
    "allowJs": true,
    "outDir": "./lib",
    "strict": true
  },
  "include": ["src"],
  "exclude": ["node_modules", "**/__tests__/*","lib","example"],
  "types": [
    "webpack-env"
  ],
}
