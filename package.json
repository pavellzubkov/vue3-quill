    "lint": "vue-cli-service lint",
    "lib": "vue-cli-service build --target lib --name vue3-quill --dest lib src/index.js"
  },
  "files": [
    "lib",
    "src"
  ],
  "dependencies": {
    "core-js": "^3.6.5",
    "quill": "^1.3.7",
    "vue": "^3.2.26"
  },
  "devDependencies": {
    "@types/quill": "^2.0.9",
    "@vue/cli-plugin-babel": "~5.0.0-rc.0",
    "@vue/cli-plugin-eslint": "~5.0.0-rc.0",
    "@vue/cli-service": "~5.0.0-rc.0",
    "@vue/cli-plugin-typescript": "~5.0.0-rc.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^9.0.1",
    "@vue/compiler-sfc": "^3.2.21",
    "eslint": "^8.6.0",
    "eslint-plugin-vue": "^8.2.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.5.1",
    "@typescript-eslint/eslint-plugin": "^5.9.0",
    "@typescript-eslint/parser": "^5.3.0",
    "@typescript/lib-dom": "npm:@types/web",
    "babel-eslint": "^10.1.0",
    "typescript": "4.5.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/flyween/vue3-quill.git"
  },
  "homepage": "https://flyween.github.io/vue3-quill",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/flyween/vue3-quill/issues"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "directories": {
    "example": "example",
    "lib": "lib"
  },
  "author": ""
